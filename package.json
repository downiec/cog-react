{
  "name": "esgf-subscriptions",
  "version": "0.4.2",
  "description": "A package that provides the front-end components for ESGF subscription features.",
  "homepage": "https://github.com/downiec/esgf-subscriptions",
  "bugs": {
    "url": "https://github.com/downiec/esgf-subscriptions/issues"
  },
  "license": "BSD-3-Clause",
  "author": "ESGF Dev Team",
  "files": [
    "pkgOutput/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "main": "pkgOutput/index.js",
  "types": "pkgOutput/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/downiec/esgf-subscriptions.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prepare": "tsc -p npm_publish_tsconfig.json && echo 'Finished building NPM package'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "COG_build": ". ./COG_start.sh --build",
    "COG": ". ./COG_start.sh",
    "build-json": "node ./src/data/jsonGenerator.js",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --fix --no-error-on-unmatched-pattern",
    "lint-fix": "npx prettier-tslint fix '**/*.ts'",
    "lint-check": "npx prettier-tslint check '**/*.ts'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "4.6.6",
    "chroma-js": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "react-select": "^3.1.0",
    "react-windowed-select": "^2.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^11.1.0",
    "@types/chroma-js": "2.1.0",
    "@types/jest": "22.2.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-select": "^3.0.21",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "6.8.0",
    "babel-eslint": "10.1.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.1.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!src/index.tsx",
      "!**/serviceWorker.js",
      "!**/react-app-env.d.ts",
      "!**/lib/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "./src/components/App/App.tsx": {
        "lines": 100
      }
    }
  }
}